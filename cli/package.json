        "@dhis2/cli-helpers-engine": "^1.4.2",
        "@dhis2/d2-i18n": "^1.0.5",
        "@dhis2/ui-core": "^3.11.0",
        "archiver": "^3.1.1",
        "babel-jest": "^24.9.0",
        "chalk": "^2.4.2",
        "classnames": "^2.2.6",
        "dotenv": "^8.1.0",
